+package com.sparta.mc;
+import org.junit.jupiter.api.Assertions;
+import org.junit.jupiter.api.DisplayName;
+import org.junit.jupiter.api.Test;
+
+public class AppTest {
+
+    @Test
+    public void testCreateListofEmployees() {
+        String[] employees = {"123,Mr.,John,D,Doe,M,johndoe@example.com"};
+        List<Employee> employeeList = App.createListofEmployees(employees);
+        Assertions.assertEquals(1, employeeList.size());
+        Employee employee = employeeList.get(0);
+        Assertions.assertEquals(123, employee.getEmpID());
+        Assertions.assertEquals("Mr.", employee.getNamePrefix());
+        Assertions.assertEquals("John", employee.getFirstName());
+        Assertions.assertEquals('D', employee.getMiddleInitial());
+        Assertions.assertEquals("Doe", employee.getLastName());
+        Assertions.assertEquals('M', employee.getGender());
+        Assertions.assertEquals("johndoe@example.com", employee.getEMail());
+    }
+}
